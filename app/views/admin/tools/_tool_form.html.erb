<%= form_for [:admin, @tool] do |f|  %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :quantity %>
  <%= f.text_field :quantity %>
  <%= f.collection_select :category_id, @categories, :id, :name%>
  <%= f.collection_select :user_id, @users, :id, :username %>
  <%= f.submit %>
<% end %>
